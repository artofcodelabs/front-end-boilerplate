{
  "private": true,
  "name": "front-end-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate code for a multi-static-page website powered by modern and fancy tools",
  "scripts": {
    "build": "webpack --mode=production --node-env=production -c webpack.prod.js --progress",
    "build-dev": "webpack -c webpack.dev.js --progress",
    "clear": "./bin/clear.sh",
    "eslint": "eslint src __tests__ ./*.js --fix",
    "lint-staged": "lint-staged",
    "start": "node dev/server.js --env=development",
    "serve": "node dev/server.js",
    "stylelint": "stylelint src/css --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "Zbigniew Humeniuk <hello@artofcode.co>",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "core-js": "^3.6.4",
    "loco-js-model": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-prop-types": "^1.0.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.20.6",
    "express": "^4.17.1",
    "husky": "^3.1.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^27.0.6",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.2.15",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.3.2",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^22.0.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
